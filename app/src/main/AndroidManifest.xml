<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.swish">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.example.user.swish.swish"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".FullAnswer"
            android:label="@string/full"
            android:parentActivityName=".swish">

        </activity>

        <activity
            android:name=".SplitAnswer"
            android:label="@string/split"
            android:parentActivityName=".swish">

        </activity>


    </application>

</manifest>
